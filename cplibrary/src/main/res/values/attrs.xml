<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- ******************************侧滑删除Created by guanaj on .EasySwipeMenuLayout******************************  -->
    <declare-styleable name="cp_EasySwipeMenuLayout">
        <attr name="cp_leftMenuView" format="reference" />
        <attr name="cp_rightMenuView" format="reference" />
        <attr name="cp_contentView" format="reference" />
        <attr name="cp_canRightSwipe" format="boolean" />
        <attr name="cp_canLeftSwipe" format="boolean" />
        <attr name="cp_fraction" format="float" />
    </declare-styleable>

    <!-- ******************************懒加载 LazyViewPager******************************  -->
    <declare-styleable name="cp_LazyViewPager">
        <attr name="cp_init_lazy_item_offset" format="float" />
    </declare-styleable>

    <!-- ****************************** 轮播RollViewPager******************************  -->
    <declare-styleable name="cp_RollViewPager">
        <attr name="cp_rollviewpager_hint_mode">
            <enum name="point" value="0" />
            <enum name="number" value="1" />
        </attr>
        <attr name="cp_rollviewpager_hint_gravity">
            <enum name="left" value="0" />
            <enum name="center" value="1" />
            <enum name="right" value="2" />
        </attr>
        <attr name="cp_rollviewpager_hint_paddingRight" format="dimension" />
        <attr name="cp_rollviewpager_hint_paddingLeft" format="dimension" />
        <attr name="cp_rollviewpager_hint_paddingTop" format="dimension" />
        <attr name="cp_rollviewpager_hint_paddingBottom" format="dimension" />
        <attr name="cp_rollviewpager_play_delay" format="integer" />
        <attr name="cp_rollviewpager_hint_color" format="color" />
        <attr name="cp_rollviewpager_hint_alpha" format="integer" />
    </declare-styleable>

    <!--**********************SwipeBackLayout侧滑返回效果********************** -->
    <declare-styleable name="cp_SwipeBackLayout">
        <attr name="cp_edge_size" format="dimension" />
        <attr name="cp_edge_flag">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
            <enum name="bottom" value="2" />
            <enum name="all" value="3" />
        </attr>
        <attr name="cp_shadow_left" format="reference" />
        <attr name="cp_shadow_right" format="reference" />
        <attr name="cp_shadow_bottom" format="reference" />
    </declare-styleable>
    <attr name="cp_SwipeBackLayoutStyle" format="reference" />

    <!--****************************  StateLayout****************************** -->
    <declare-styleable name="cp_StateLayout">
        <!-- 错误提示图标 -->
        <attr name="cp_errorImg" format="reference" />
        <!-- 错误提示文字 -->
        <attr name="cp_errorText" format="string" />
        <!-- 空数据提示图标 -->
        <attr name="cp_emptyImg" format="reference" />
        <!-- 空数据提示文字 -->
        <attr name="cp_emptyText" format="string" />
        <!-- 没有网络提示图标 -->
        <attr name="cp_noNetworkImg" format="reference" />
        <!-- 没有网络提示文字 -->
        <attr name="cp_noNetworkText" format="string" />
        <!-- 超时提示图标 -->
        <attr name="cp_timeOutImg" format="reference" />
        <!-- 超时提示文字 -->
        <attr name="cp_timeOutText" format="string" />
        <!-- 加载提示文字 -->
        <attr name="cp_loadingTexts" format="string" />
        <!-- 登录提示图标 -->
        <attr name="cp_loginImg" format="reference" />
        <!-- 登录提示文字 -->
        <attr name="cp_loginText" format="string" />
        <!-- 加载提示文字 -->
        <attr name="cp_loadingText" format="string" />
    </declare-styleable>

    <!--****************************  手机屏幕签字控件****************************  -->
    <declare-styleable name="cp_HandWriteView">
        <!--画笔颜色-->
        <attr name="cp_paintColor" format="color" />
        <!--画笔最大宽度控制字的粗细-->
        <attr name="cp_paintMaxWidth" format="dimension" />
        <attr name="cp_paintMinWidth" format="dimension" />
    </declare-styleable>

    <!--****************************  LoadingButton、LoadingView****************************  -->
    <declare-styleable name="cp_LoadingButton">
        <attr name="cp_lb_btnColor" format="reference|color" />
        <attr name="cp_lb_btnText" format="reference|string" />
        <attr name="cp_lb_textColor" format="reference|string" />
        <attr name="cp_lb_resetAfterFailed" format="boolean" />
        <attr name="cp_lb_btnRippleColor" format="reference|color" />
        <attr name="cp_lb_btnRippleAlpha" format="float" />
    </declare-styleable>
    <declare-styleable name="cp_LoadingView">
        <attr name="cp_loadingTxt" format="string" />
        <attr name="cp_loadingTextAppearance" format="reference" />
    </declare-styleable>
    <!--**************************** 星星评分控件 **************************** -->
    <declare-styleable name="cp_MaterialRatingBar">
        <!--选中的实心星星的颜色：cp_mrb_progressTint-->
        <attr name="cp_mrb_progressTint" format="color|reference" />
        <attr name="cp_mrb_progressTintMode" format="enum">
            <enum name="src_over" value="3" />
            <enum name="src_in" value="5" />
            <enum name="src_atop" value="9" />
            <enum name="multiply" value="14" />
            <enum name="screen" value="15" />
            <enum name="add" value="16" />
        </attr>
        <attr name="cp_mrb_secondaryProgressTint" format="color|reference" />
        <attr name="cp_mrb_secondaryProgressTintMode" format="enum">
            <enum name="src_over" value="3" />
            <enum name="src_in" value="5" />
            <enum name="src_atop" value="9" />
            <enum name="multiply" value="14" />
            <enum name="screen" value="15" />
            <enum name="add" value="16" />
        </attr>
        <!--未选中的空心星星颜色：cp_mrb_progressBackgroundTint-->
        <attr name="cp_mrb_progressBackgroundTint" format="color|reference" />
        <attr name="cp_mrb_progressBackgroundTintMode" format="enum">
            <enum name="src_over" value="3" />
            <enum name="src_in" value="5" />
            <enum name="src_atop" value="9" />
            <enum name="multiply" value="14" />
            <enum name="screen" value="15" />
            <enum name="add" value="16" />
        </attr>
        <attr name="cp_mrb_indeterminateTint" format="color|reference" />
        <attr name="cp_mrb_indeterminateTintMode" format="enum">
            <enum name="src_over" value="3" />
            <enum name="src_in" value="5" />
            <enum name="src_atop" value="9" />
            <enum name="multiply" value="14" />
            <enum name="screen" value="15" />
            <enum name="add" value="16" />
        </attr>
        <!--cp_mrb_fillBackgroundStars：true 没有选中的星星为实心，false：则为空心-->
        <attr name="cp_mrb_fillBackgroundStars" format="boolean" />
    </declare-styleable>
    <!--************** Created by Allen:github地址：https://github.com/lygttpod/AndroidCustomView -CircleProgressBarView**************-->
    <declare-styleable name="cp_CircleProgressBarView">
        <attr name="cp_circleBgStrokeWidth" format="dimension" /> <!--进度条 背景线宽-->
        <attr name="cp_progressStrokeWidth" format="dimension" /><!--进度条 进度线宽-->
        <attr name="cp_circleBgColor" format="color" /><!--进度条 背景线底色颜色-->
        <attr name="cp_progressColor" format="color" /><!--进度条 进度线的颜色-->
        <attr name="cp_circleAnimationDuration" format="integer" /><!--动画执行时间：单位ms-->
        <attr name="cp_isDrawCenterProgressText" format="boolean" /><!--是否显示圆中心的文字-->
        <attr name="cp_centerProgressTextColor" format="color" /> <!--圆中心文字的颜色-->
        <attr name="cp_centerProgressTextSize" format="dimension" /><!--圆中心文字的大小-->
    </declare-styleable>

    <declare-styleable name="cp_horizontalProgressBar">
        <attr name="cp_progressWidth" format="dimension" /><!--进度条 进度线宽-->
        <attr name="cp_bgColor" format="color" /><!--进度条 背景线底色颜色-->
        <attr name="cp_hProgressColor" format="color" /><!--进度条 进度线的颜色-->
        <attr name="cp_duration" format="integer" /><!--动画执行时间：单位ms-->
        <attr name="cp_isDrawText" format="boolean" /><!--是否进度文字-->
        <attr name="cp_textSize" format="dimension" /><!--进度文字的大小-->
    </declare-styleable>
    <declare-styleable name="cp_LoadingLineView">
        <attr name="cp_lineWidth" format="dimension" /><!--进度条线宽-->
        <attr name="cp_linePgColor" format="reference|color" /> <!--进度线的颜色-->
        <attr name="cp_lineBgColor" format="reference|color" /><!--背景颜色-->
    </declare-styleable>
    <!--************** LoadingCircleVieww**************-->

    <declare-styleable name="cp_LoadingCircleView">
        <attr name="cp_circlePgColor" format="reference|color" />
        <attr name="cp_circleBgdColor" format="reference|color" />
    </declare-styleable>
    <!--************** PayPsdInputView**************-->
    <declare-styleable name="cp_PayPsdInputView">
        <!--
        cp_maxCount：密码框位数
        cp_circleColor：密码点的颜色
        cp_bottomLineColor:线型密码框的颜色
        cp_radius:密码点的大小
        cp_divideLineWidth：矩形密码框中间竖直分割线宽
        cp_divideLineColor ：矩形密码框中间竖直分割线颜色
        cp_rectAngle：整矩形密码框和单个小密码框的四个角的角度，
        cp_focusedColor焦点框的框线颜色，
        cp_psdType：weChat类似微信巨矩形密码输入框，bottomLine只有一条线的线型密码输入框
       -->
        <attr name="cp_maxCount" format="integer" />
        <attr name="cp_circleColor" format="color" />
        <attr name="cp_bottomLineColor" format="color" />
        <attr name="cp_radius" format="dimension" />
        <attr name="cp_divideLineWidth" format="dimension" />
        <attr name="cp_divideLineColor" format="color" />
        <attr name="cp_rectAngle" format="dimension" />
        <attr name="cp_focusedColor" format="color" />
        <attr name="cp_psdType" format="enum">
            <enum name="weChat" value="0" />
            <enum name="bottomLine" value="1" />
        </attr>
    </declare-styleable>
</resources>